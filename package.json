  },
  "homepage": "https://github.com/advanced-security/maven-dependency-tree-action#readme",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@github/dependency-submission-toolkit": "^1.2.8",
    "commander": "^9.4.0",
    "packageurl-js": "^0.0.7"
  },
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/jest": "^28.1.6",
    "@vercel/ncc": "^0.34.0",
    "chai": "^4.3.6",
    "jest": "^28.1.3",
    "pkg": "^5.8.0",
